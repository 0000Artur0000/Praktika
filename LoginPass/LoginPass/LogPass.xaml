             xmlns:local="clr-namespace:LoginPass"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Width="300" Height="130">
    <Grid x:Name="W10" HorizontalAlignment="Left" Height="130" VerticalAlignment="Center" Width="300">
        <Label x:Name="w2" Content=":" HorizontalAlignment="Center" VerticalAlignment="Center" Width="280" Height="34" VerticalContentAlignment="Center" FontSize="18" Margin="10,10,10,88"/>
        <TextBox x:Name="w13" HorizontalAlignment="Left" Margin="10,44,0,52" TextWrapping="Wrap" Width="280" VerticalContentAlignment="Center" FontSize="16" Background="#FFD1EEFC"/>

        <Label x:Name="w12" Content="" VerticalContentAlignment="Center" Margin="10,80,10,30" HorizontalContentAlignment="Center" Padding="0" Foreground="White" Panel.ZIndex="-1" Visibility="Hidden"/>
        <Polygon x:Name="w11" Fill="#FFE0A9AF" Points="2,0 20,20 255,20 275,0" Margin="10,80,10,30" Panel.ZIndex="-2" Visibility="Hidden" />

    </Grid>
